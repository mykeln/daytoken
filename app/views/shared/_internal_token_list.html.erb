<div class="row demo-samples">
      <div class="todo mrm">
        <ul>
          <% @user.ribbits.each do |ribbit| %>
            <%#= render :partial => 'token_item' %>
            <li class="todo">
              <div class="todo-icon"><%= time_ago_in_words(ribbit.created_at) %></div>
              <div class="todo-content">
                <h4 class="todo-name">
                  <%= ribbit.content %>
                </h4>
                <%= time_ago_in_words(ribbit.created_at) %>
              </div>
            </li>
          <% end %>
        </ul>
      </div>
  </div>